         <TextField fx:id="txtNum01" layoutX="6.0" layoutY="27.0" prefHeight="26.0" prefWidth="188.0" />
         <Label layoutX="9.0" layoutY="74.0" text="Número 02" />
         <TextField fx:id="txtNum02" layoutX="8.0" layoutY="92.0" prefHeight="26.0" prefWidth="185.0" />
         <Label fx:id="res" layoutX="13.0" layoutY="132.0" text="Resultado:" />
         <Button fx:id="btnSumar" layoutX="8.0" layoutY="159.0" mnemonicParsing="false" onMouseClicked="#btnSumarOnClick" prefHeight="26.0" prefWidth="184.0" text="Sumar" />
